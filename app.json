{
  "expo": {
    "name": "FarmersMarketApp",
    "slug": "FarmersMarketApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.farmersmarketapp",
      "config": {
        "googleMapsApiKey": "YOUR_IOS_API_KEY"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.farmersmarketapp",
      "config": {
        "googleMaps": {
          "apiKey": "YOUR_ANDROID_API_KEY"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Farmers Market App to use your location."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Farmers Market App to access your photos to share market pictures.",
          "cameraPermission": "Allow Farmers Market App to access your camera to take market pictures."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Farmers Market App to access your camera to capture and share market experiences."
        }
      ]
    ],
    "extra": {
      "supabaseUrl": "https://dfdhblstxjjnosdsziba.supabase.co",
      "supabaseAnonKey": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZSIsInJlZiI6ImRmZGhibHN0eGpqbm9zZHN6aWJhIiwicm9sZSI6ImFub24iLCJpYXQiOjE3NTAxNzgxMjgsImV4cCI6MjA2NTc1NDEyOH0.2QSeH2orryYFGUBXzBeDzxbweY5zejcKvgLzv0iHMYY",
      "skipLaunchAnimation": false
    }
  }
}